{
  "name": "Job Application Tracker",
  "description": "Track your job applications using GitHub projects",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "icons/icon48.png"
  },
  "permissions": ["contextMenus", "scripting", "activeTab", "storage"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "contextMenu.js"
  }
}
